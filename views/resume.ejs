<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Fontawesome CDN link here -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
        integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="/stylesheet/resume.css">
    <title>Resume UI</title>
</head>

<body>
    <!-- Main container starts here -->
    <div class="container">

        <!-- Left panel starts here -->
        <div class="left__side">
            <div class="left__triangle"></div>
            <div class="profile">
                <div class="imgBox">
                    <!-- Profile image -->
                    <img src="<%= profile.profileImage %>" alt="profile-image">
                </div>
            </div>

            <!-- Contact info section -->
            <div class="contactInfo">
                <!-- Contact box: Contact Me -->
                <div class="contact-box contact-box-top yellow-flag-border-left">
                    <div class="left-main-icon">
                        <span class="left-main-icon"><i class="fa-solid fa-user"></i></span>
                    </div>
                    <div class="left-main-head">
                        <h1 class="title">Contact Me</h1>
                    </div>
                </div>

                <!-- Contact box: Phone -->
                <div class="contact-box yellow-flag-border-left-small">
                    <div class="left-small-icon">
                        <span class="left-main-icon"><i class="fa-solid fa-phone"></i></span>
                    </div>
                    <div class="left-contact-details">
                        <p><%= profile.contact %></p>
                        
                    </div>
                </div>

                <!-- Contact box: Website and Email -->
                <div class="contact-box yellow-flag-border-left-small">
                    <div class="left-small-icon">
                        <span class="left-main-icon"><i class="fa-solid fa-globe"></i></span>
                    </div>
                    <div class="left-contact-details">
                        <p><a href="<%= profile.linkedin %>">Linkedin: <%= profile.linkedin %></a></p>
                        <p><a href="mailto:<%= profile.email %>"><%= profile.email %></a></p>
                    </div>
                </div>

                <!-- Contact box: Address -->
                <div class="contact-box yellow-flag-border-left-small">
                    <div class="left-small-icon">
                        <span class="left-main-icon"><i class="fa-solid fa-location-dot"></i></span>
                    </div>
                    <div class="left-contact-details">
                        <p><a href="<%= profile.github %>">github:<%= profile.github %></a></p>
                       
                    </div>
                </div>
            </div>

            <!-- References section -->
            <div class="contactInfo">
                <!-- Contact box: References -->
                <div class="contact-box contact-box-top ">
                    <div class="left-main-icon">
                        <span class="left-main-icon"><i class="fa-solid fa-user-group"></i></span>
                    </div>
                    <div class="left-main-head">
                        <h1 class="title">References</h1>
                    </div>
                </div>

                <!-- Individual contact details for references -->
                <div class="contact-details">
                    <h2><%=profile.name%></h2>
                    <p><%=profile.address%></p>
                    <p><a href="tel:<%= profile.contact %>">tel:<%= profile.contact %></a></p>
                    <p><a href="mailto:<%=profile.email%>">Email: <%=profile.email%></a></p>
                </div>
              

               
            </div>

            <!-- Education section -->
            <div class="contactInfo no-border-bottom">
                <!-- Contact box: References -->
                <div class="contact-box contact-box-top ">
                    <div class="left-main-icon">
                        <span class="left-main-icon"><i class="fa-brands fa-google-scholar"></i></span>
                    </div>
                    <div class="left-main-head">
                        <h1 class="title">Education</h1>
                    </div>
                </div>
                <!-- Individual contact details for education -->
                <div class="contact-details">
                    <h2><%=profile.Education%></h2>
                    <p><%=profile.address%></p>
                    <p><a href="tel:<%= profile.contact %>">tel:<%= profile.contact %></a></p>
                    <p><a href="mailto:<%=profile.email%>">Email: <%=profile.email%></a></p>
                </div>
              

               
            </div>
        </div>
        <!-- Left panel ends here -->

        <!-- Right panel starts here -->
        <div class="right__side">
            <div class="right__triangle"></div>
            <div class="intro">
                <h1><%= profile.name %></h1>
                <p><%=profile.expertise%></p>
            </div>

            <!-- About section -->
            <div class="about">
                <h2 class="title2 about-head">
                    <span class="right-icon"><i class="fa-solid fa-graduation-cap"></i></span>About Me
                </h2>
                <div class="box">
                    <p class="right-text"><%= profile.about %></p>
                    </div>
                </div>
    
                <!-- Experience section -->
                <div class="experience">
                    <h2 class="title2">
                        <span class="right-icon"><i class="fa-solid fa-briefcase"></i></span>Job Experience
                    </h2>
                    <!-- Individual job experiences -->
                   
                   
                    <div class="box">
                        <p class="right-text job-desc"><%= profile.experiences %></p>
                    </div>
    
                   
                </div>
    
                <!-- Skills section -->
                <div class="skills">
                    <h2 class="title2 skills-head">
                        <span class="right-icon"><i class="fa-solid fa-chart-simple"></i></span>Skills
                    </h2>
                    <!-- Individual skills with percentages -->
                    <div class="box">
                        <div class="text">
                            <h4><%=profile.Skill1%></h4>
                        </div>
                       
                        <div class="text">
                            <h4><%=profile.Skill2%></h4>
                        </div>
                        
                    </div>
                    <div class="box">
                        <div class="text">
                            <h4><%=profile.Skill3%></h4>
                        </div>
                       
                        <div class="text">
                            <h4><%=profile.Skill4%></h4>
                        </div>
                        
                    </div>
                   
                </div>
            </div>
            <!-- Right panel ends here -->
        </div>
        <!-- Main container ends here -->
    </body>
    
    </html>
    